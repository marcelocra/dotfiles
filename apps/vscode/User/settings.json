  "window.title": "[${rootNameShort}] ${activeEditorMedium}",
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "all",
  "window.newWindowProfile": "Default",
  "window.menuBarVisibility": "hidden",
