{
	// Place your snippets for properties here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"linguist detectable": {
		"prefix": "lde",
		"body": [
			"${1:linguist-detectable}"
		]
	},
	"linguist documentation": {
		"prefix": "ldo",
		"body": [
			"${1:linguist-documentation}"
		]
	},
	"linguist generated": {
		"prefix": "lge",
		"body": [
			"${1:linguist-generated}"
		]
	},
	"linguist language": {
		"prefix": "lla",
		"body": [
			"${1:linguist-language=${0:name}}"
		]
	},
	"linguist vendored": {
		"prefix": "lve",
		"body": [
			"${0:linguist-vendored}"
		]
	},
	"GitHub linguist examples to use as reference at the top of `.gitattributes` files": {
		"prefix": "linguist",
		"body": [
			"# Linguist examples",
			"# -----------------",
			"# linguist-detectable        languages.yml         Included in stats, even if language's type is data or prose",
			"# linguist-documentation     documentation.yml     Excluded from stats",
			"# linguist-generated         generated.rb          Excluded from stats, hidden in diffs",
			"# linguist-language=name     languages.yml         Highlighted and classified as name",
			"# linguist-vendored          vendor.yml            Excluded from stats",
			"",
			"$0"
		]
	},
	"Creates a Git LFS section for use in `.gitattributes` files": {
		"prefix": "lfs",
		"body": [
			"# Git LFS",
			"# -------",
			"# Examples:",
			"# *.png filter=lfs diff=lfs merge=lfs -text",
			"# *.ico filter=lfs diff=lfs merge=lfs -text",
			"",
			"$0"
		]
	},
}