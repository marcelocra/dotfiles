  "template de frontmatter para posts no blog": {
    "prefix": [
      "blog",
      "post"
    ],
    "body": [
      "---",
      "title: ${1:Post title here}",
      "author: ${2:Author name}",
      "description: >",
      "  ${3:Post description here}",
      "createdDate: \"$CURRENT_YEAR-$CURRENT_MONTH-${CURRENT_DATE}T$CURRENT_HOUR:$CURRENT_MINUTE:${CURRENT_SECOND}${CURRENT_TIMEZONE_OFFSET}\"",
      "pubDate: \"$CURRENT_YEAR-$CURRENT_MONTH-${CURRENT_DATE}T$CURRENT_HOUR:$CURRENT_MINUTE:${CURRENT_SECOND}${CURRENT_TIMEZONE_OFFSET}\"",
      "updatedDate: \"$CURRENT_YEAR-$CURRENT_MONTH-${CURRENT_DATE}T$CURRENT_HOUR:$CURRENT_MINUTE:${CURRENT_SECOND}${CURRENT_TIMEZONE_OFFSET}\"",
      "tags:",
      "  - ${4:tag 1}",
      "---",
      "",
      "$0"
    ]
  },
  // Creates a new heading with current date and time.
  "alt+enter": {
    "prefix": [
      "ht",
      "h"
    ],
    "body": [
      "",
      "",
      "# [${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}] $0",
      ""
    ]
  },
  // Creates a new header for task with current date and time.
  "ctrl+enter": {
    "prefix": [
      "task",
      "t",
    ],
    "body": [
      "",
      "",
      "# [${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}] [${1:todo}] [${2:p2}] $0",
      "",
    ]
  },
  // Creates a new topic, with current date and time.
  "alt+shift+enter": {
    "prefix": [
      "topic",
      "to",
    ],
    "body": [
      "## [TOPIC] $0",
      "",
      "_Added: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}_",
      ""
    ]
  },
  "new day separator": {
    "prefix": [
      "nd",
      "sep"
    ],
    // Make it really long, so it disappears in the symbols view.
    "body": "# ------------------------------------------------------------------------------------------------------------------------------------------ new day!"
  },
  ". bullet for new task": {
    "prefix": [
      ".."
    ],
    "body": [
      ". $1",
      "..$0"
    ]
  },
  ">>>>> NEXT snippet above. Ignore this, it is not a snippet.": {
    "body": "",
  }
}