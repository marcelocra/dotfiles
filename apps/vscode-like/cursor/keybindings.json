// Place your key bindings in this file to override the defaults
[
  {
      "key": "alt+d",
      "command": "editor.action.duplicateSelection",
      "when": "editorTextFocus"
  },
  {
      "key": "ctrl+q",
      "command": "workbench.action.quickOpenNavigateNextInViewPicker",
      "when": "inQuickOpen && inViewsPicker && !terminalFocus"
  },
  {
      "key": "ctrl+q",
      "command": "-workbench.action.quickOpenNavigateNextInViewPicker",
      "when": "inQuickOpen && inViewsPicker"
  },
  {
      "key": "ctrl+q",
      "command": "workbench.action.quickOpenView",
      "when": "!terminalFocus"
  },
  {
      "key": "ctrl+q",
      "command": "-workbench.action.quickOpenView"
  },
  {
      "key": "ctrl+p",
      "command": "workbench.action.quickOpenNavigateNextInFilePicker",
      "when": "inFilesPicker && inQuickOpen && !terminalFocus"
  },
  {
      "key": "ctrl+p",
      "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
      "when": "inFilesPicker && inQuickOpen"
  },
  {
      "key": "ctrl+p",
      "command": "notebook.centerActiveCell",
      "when": "notebookEditorFocused && !terminalFocus"
  },
  {
      "key": "ctrl+p",
      "command": "-notebook.centerActiveCell",
      "when": "notebookEditorFocused"
  },
  {
      "key": "ctrl+p",
      "command": "workbench.action.quickOpen",
      "when": "!terminalFocus"
  },
  {
      "key": "ctrl+p",
      "command": "-workbench.action.quickOpen"
  },
  {
      "key": "ctrl+p",
      "command": "-expandLineSelection",
      "when": "textInputFocus"
  },
  {
      "key": "ctrl+l",
      "command": "workbench.action.focusBrowserLocationBar",
      "when": "browserEditorActive && cursor.browserTabEnabled && !terminalFocus"
  },
  {
      "key": "ctrl+l",
      "command": "-workbench.action.focusBrowserLocationBar",
      "when": "browserEditorActive && cursor.browserTabEnabled"
  },
  {
      "key": "ctrl+l",
      "command": "aichat.newchataction",
      "when": " !terminalFocus"
  },
  {
      "key": "ctrl+l",
      "command": "-aichat.newchataction"
  },
  {
      "key": "ctrl+l",
      "command": "composer.sendToAgent",
      "when": "editorHasPromptBar && editorPromptBarFocused && !terminalFocus"
  },
  {
      "key": "ctrl+l",
      "command": "-composer.sendToAgent",
      "when": "editorHasPromptBar && editorPromptBarFocused"
  },
  {
      "key": "alt+g",
      "command": "workbench.view.scm",
      "when": "workbench.scm.active"
  },
  {
      "key": "alt+r",
      "command": "workbench.view.explorer",
      "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
      "key": "alt+a",
      "command": "-editor.action.accessibilityHelpConfigureAssignedKeybindings",
      "when": "accessibilityHelpIsShown && accessibleViewHasAssignedKeybindings"
  },
  {
      "key": "alt+a",
      "command": "aichat.newchataction",
      "when": "!terminalFocus"
  },
  {
      "key": "alt+a",
      "command": "composer.sendToAgent",
      "when": "editorHasPromptBar && editorPromptBarFocused && !terminalFocus"
  },
  {
      "key": "alt+k",
      "command": "workbench.action.nextEditor"
  },
  {
      "key": "alt+j",
      "command": "workbench.action.previousEditor"
  },
  {
      "key": "shift+alt+j",
      "command": "workbench.action.moveEditorLeftInGroup"
  },
  {
      "key": "shift+alt+k",
      "command": "workbench.action.moveEditorRightInGroup"
  },
  {
      "key": "ctrl+alt+oem_3",
      "command": "workbench.action.toggleMaximizedPanel"
  },
  {
      "key": "alt+e",
      "command": "workbench.action.quickOpenPreviousRecentlyUsedAgent",
      "when": "agentsPaneFocused && !isInBackgroundComposerWindow || composerFocused && !isInBackgroundComposerWindow"
  },
  {
      "key": "alt+e",
      "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
      "when": "activeEditor && !activeEditorGroupEmpty && !agentsPaneFocused && !auxiliaryBarFocus && !composerFocused && !panelFocus && !sideBarFocus && !terminalFocus || editorFocus && !activeEditorGroupEmpty && !agentsPaneFocused && !auxiliaryBarFocus && !composerFocused && !panelFocus && !sideBarFocus && !terminalFocus"
  },
  {
      "key": "alt+e",
      "command": "workbench.action.quickOpenNavigateNextInAgentsPicker",
      "when": "inAgentsMRUPicker && inQuickOpen"
  },
  {
      "key": "alt+e",
      "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
      "when": "inEditorsPicker && inQuickOpen"
  },
  {
      "key": "alt+z",
      "command": "workbench.action.toggleZenMode",
      "when": "!isAuxiliaryWindowFocusedContext"
  },
  {
      "key": "alt+z",
      "command": "-workbench.action.terminal.sizeToContentWidth",
      "when": "terminalFocus && terminalHasBeenCreated && terminalIsOpen || terminalFocus && terminalIsOpen && terminalProcessSupported"
  },
  {
      "key": "shift+alt+w",
      "command": "editor.action.toggleWordWrap"
  },
  {
      "key": "alt+z",
      "command": "-editor.action.toggleWordWrap"
  },
  {
      "key": "alt+x",
      "command": "workbench.view.extensions",
      "when": "viewContainer.workbench.view.extensions.enabled"
  },
  {
      "key": "ctrl+r",
      "command": "workbench.action.gotoSymbol",
      "when": "!accessibilityHelpIsShown && !accessibleViewIsShown"
  },
  {
      "key": "ctrl+tab",
      "command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
      "when": "inEditorsPicker && inQuickOpen"
  },
  {
      "key": "ctrl+tab",
      "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
      "when": "activeEditor && !activeEditorGroupEmpty && !agentsPaneFocused && !auxiliaryBarFocus && !composerFocused && !panelFocus && !sideBarFocus && !terminalFocus || editorFocus && !activeEditorGroupEmpty && !agentsPaneFocused && !auxiliaryBarFocus && !composerFocused && !panelFocus && !sideBarFocus && !terminalFocus"
  },
  {
      "key": "ctrl+shift+tab",
      "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
      "when": "activeEditor && !activeEditorGroupEmpty && !agentsPaneFocused && !auxiliaryBarFocus && !composerFocused && !panelFocus && !sideBarFocus && !terminalFocus || editorFocus && !activeEditorGroupEmpty && !agentsPaneFocused && !auxiliaryBarFocus && !composerFocused && !panelFocus && !sideBarFocus && !terminalFocus"
  },
  {
      "key": "ctrl+shift+tab",
      "command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker",
      "when": "inEditorsPicker && inQuickOpen"
  },
  {
      "key": "shift+alt+e",
      "command": "workbench.action.quickOpenLeastRecentlyUsedAgent",
      "when": "agentsPaneFocused && !isInBackgroundComposerWindow || composerFocused && !isInBackgroundComposerWindow"
  },
  {
      "key": "shift+alt+e",
      "command": "workbench.action.quickOpenNavigatePreviousInAgentsPicker",
      "when": "inAgentsMRUPicker && inQuickOpen"
  },
  {
      "key": "ctrl+shift+tab",
      "command": "workbench.action.previousEditor"
  },
  {
      "key": "ctrl+tab",
      "command": "workbench.action.nextEditor"
},
{
    "key": "alt+b",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!isAuxiliaryWindowFocusedContext"
},
{
    "key": "shift+alt+b",
    "command": "workbench.action.toggleAuxiliaryBar",
    "when": "!isAuxiliaryWindowFocusedContext"
},
{
    "key": "ctrl+m ctrl+k",
    "command": "workbench.action.openGlobalKeybindingsFile"
},
{
    "key": "ctrl+i",
    "command": "aipopup.action.modal.generate",
    "when": "editorFocus && !composerBarIsVisible"
},
{
    "key": "ctrl+k",
    "command": "-aipopup.action.modal.generate",
    "when": "editorFocus && !composerBarIsVisible"
},
{
    "key": "ctrl+i",
    "command": "cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
},
{
    "key": "ctrl+k",
    "command": "-cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalPromptBarVisible || terminalProcessSupported && terminalPromptBarVisible"
},
{
    "key": "ctrl+i",
    "command": "editor.action.inlineDiffs.focusEditor",
    "when": "editorHasPromptBar && editorPromptBarFocused"
},
{
    "key": "ctrl+k",
    "command": "-editor.action.inlineDiffs.focusEditor",
    "when": "editorHasPromptBar && editorPromptBarFocused"
},
]
