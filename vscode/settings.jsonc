    // },
    // {
    //   "before": [" ", "e", "b"],
    //   "commands": ["fsi.SendFile"]
    // },
    // //#endregion }}}
    {
      "before": ["<c-h>"],
      "commands": ["editor.action.startFindReplaceAction"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    }
  ],
